module controlTst;
   wire [127:0] out, in, k;

   assign
     in = 'H0123456789abcdefefdcba9876543210,
     k  = 'H0f1571c947d9e8590cb7add6af7f6798;

   aes128
     main(.out(out), .in(in), .k(k));
   
   initial
     begin
	$display("in = %h", in);
	$display("k  = %h", k);
	#100 $display("out = %h", out);
     end
endmodule // controlTst
